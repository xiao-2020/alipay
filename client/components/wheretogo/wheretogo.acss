.wheretogo-box {
  position: relative; /* 有没有其实无所谓 */
  top: 0;
  left: 0;
  padding: 10rpx 30rpx;
  width: 640rpx;
  height: 180rpx;
  background-color: #ffffff;
  border-radius: 5px;
  box-shadow: 0 5rpx 10rpx 1px rgba(0, 0, 0, 0.1); 
}
.select-zone {
  width: 520rpx;
  height: 100%;
}
.startpoint, .endpoint {
  width: 100%;
  height: 50%;
}
.choicelocation {
  margin-left: 20rpx;
  width: calc(100% - 40rpx);
  border-bottom: 1rpx solid #999;
  height: 100%;
}